<%- include ('partials/header.ejs') %> <%- include ('partials/homenavbar.ejs')
%>
<body>
    <div class="container">
        <hr />
        <% articles.forEach( (article) => { %>
        <div class="card">
            <div class="card-header">
                <h2><%= article.title %></h2>
            </div>
            <div class="card-body">
                <a href="/<%= article.slug %>" class="btn btn-primary"
                    >Ler artigo</a
                >
            </div>
        </div>
        <% }) %>
        <div class="row">
            <div class="col col-12">
                <a style="float: right" href="">Próxima Página</a>
            </div>
        </div>
    </div>
</body>
<%- include ('partials/footer.ejs') %>
